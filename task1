#include <stdio.h>
#include <stdlib.h>


int main(){ 
    int n,i,A[n],B[n],C[n]; 
    scanf("%d", &n);
    for(i=0;i<n;i++)
        scanf("%d",&A[i]);
    for(i=0;i<n;i++)
        scanf("%d",&B[i]);
    for(i=0;i<n;i++)
        C[i]=A[B[i]-1];
    for(i=0;i<n;i++)
        printf("%d ",C[i]);
return 0;
}
